from operator import itemgetter

contigs = open("contigs_414722-417122_0.txt", "r")
all_contigs = {}

for line in contigs:
    line = line.rstrip("\n")
    mm = line.split("mm ")
    all_contigs[mm[0]] = mm[1]

for value in sorted(all_contigs.values(), key=int, reverse=True):
    for k, v in all_contigs.items():
        if value == v:
            print k, v
            del all_contigs[k]
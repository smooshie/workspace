from operator import itemgetter
from collections import defaultdict

all_contigs = defaultdict(int)
writeFile = open("sortedContigs.txt", "w")

with open("contigs_414722-417122_0.txt", "r") as readFile:
    for line in readFile:
        line = line.rstrip("\n")
        mm = line.split("mm ")
        all_contigs[mm[0]] = mm[1]

for key, value in sorted(all_contigs.items(), key=itemgetter(1)):
    print key, value
    
'''    for k, v in all_contigs.items():
        if value == v:
            print >> writeFile, k, v
            del all_contigs[k]
'''
writeFile.close()


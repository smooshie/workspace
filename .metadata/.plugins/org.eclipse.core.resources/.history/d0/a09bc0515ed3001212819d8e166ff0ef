with open("testi.txt", "r") as readFile:
    for line in readFile:
        if line[0] == "*":
            temp = ""
            temp += line
            nextLine = readFile.next()
            if nextLine[0] != "*":
                old = True
            else:
                old = False
        elif old == True:
            temp += line
        elif old == False:
            print "???"
        
        print temp
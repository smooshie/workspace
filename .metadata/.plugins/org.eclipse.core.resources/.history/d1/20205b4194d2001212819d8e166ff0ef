from operator import itemgetter

contigs = open("contigs_414722-417122_0.txt", "r")
all_contigs = {}

for line in contigs:
    line = line.rstrip("\n")
    mm = line.split("mm ")
    all_contigs[mm[0]] = mm[1]

for key, value in sorted(all_contigs.items(), key=int):
    print key, value
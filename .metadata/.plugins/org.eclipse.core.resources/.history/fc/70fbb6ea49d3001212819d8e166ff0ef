import glob
import os

all_contigs = {}
writeFile = open("sortedContigs.txt", "w")

with open("contigs_414722-417122_0.txt", "r") as readFile:
    for line in readFile:
        line = line.rstrip("\n")
        mm = line.split("mm ")
        all_contigs[mm[0]] = mm[1]

for value in sorted(all_contigs.values(), key=all_contigs.get, reverse=True):
    print all_contigs[value], value
    
